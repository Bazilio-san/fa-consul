{
  "name": "af-consul-ts",
  "version": "5.0.1",
  "description": "A highly specialized function library",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Viacheslav Makarov <vvmakarov@bazilio.ru>",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "test:register": "node --loader ts-node/esm ./__tests__/lib/register.ts",
    "test:deregister": "node --loader ts-node/esm ./__tests__/lib/deregister.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js __tests__/unit/",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean": "shx rm -rf dist/*",
    "build": "tsc -p tsconfig.build.json",
    "cb": "shx rm -rf dist/* && tsc -p tsconfig.build.json",
    "cbd": "shx rm -rf dist/* && tsc -p tsconfig.json"
  },
  "dependencies": {
    "af-tools-ts": "^1.1.5",
    "async-mutex": "^0.5.0",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@jest/test-sequencer": "^30.2.0",
    "@types/config": "^3.3.5",
    "@types/jest": "^29.5.6",
    "@types/node": "^24.10.1",
    "@types/xxhashjs": "^0.2.4",
    "af-logger": "^2.0.29",
    "config": "^4.1.1",
    "dotenv": "^17.2.3",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "shx": "^0.4.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "np": {
    "yarn": false,
    "contents": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bazilio-san/af-consul-ts.git"
  }
}
